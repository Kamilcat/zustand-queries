{
  "name": "zustand-queries",
  "private": true,
  "version": "0.4.6",
  "type": "module",
  "keywords": [
    "zustand-queries",
    "zustand-query",
    "zustand",
    "react-query",
    "React Query",
    "SWR",
    "RTK Query",
    "api",
    "cache",
    "useQuery",
    "useMutation",
    "useSWR"
  ],
  "description": "API integration with caching for Zustand",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "author": {
    "name": "Kamil Mindubaev",
    "email": "kamilcat@yandex.ru",
    "url": "https://t.me/kamilcat"
  },
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/Kamilcat/zustand-queries.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/Kamilcat/zustand-queries/issues"
  },
  "homepage": "https://github.com/Kamilcat/zustand-queries",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test:unit": "vitest --ui --coverage",
    "format": "prettier --write lib/",
    "coverage": "vitest run --coverage"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-legacy": "^5.4.0",
    "@vitest/coverage-istanbul": "^1.5.2",
    "@vitest/ui": "^1.4.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unicorn": "^52.0.0",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "ts-xor": "^1.3.0",
    "typescript": "^5.2.2",
    "utility-types": "^3.11.0",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.5.2"
  },
  "peerDependencies": {
    "zustand": ">=4"
  }
}
